    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.8",
    "karma-webpack": "^5.0.0",
    "mocha": "^8.3.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.8",
    "postcss-loader": "^5.2.0",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "sharp": "^0.27.2",
    "style-loader": "^2.0.0",
    "stylelint": "^13.12.0",
    "stylelint-config-standard": "^21.0.0",
    "vue-router": "^3.5.1",
    "web-ext": "^6.0.0",
    "webpack": "^5.28.0",
    "webpack-clean-plugin": "^0.2.3",
    "webpack-cli": "^4.6.0"
